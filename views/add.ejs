<%- include('./templates/header.ejs')  %>
<body>
    <%- include('./templates/navbar.ejs')  %>
        <main id="read_key">
            <h1>AÃ±adir producto</h1>
            <div id="add">
                <p>Nombre:</p>
                <input type="text" name="nombre" autofocus>
                <p>Precio:</p>
                <input type="number" name="precio">
                <p>Categoria:</p>
                <select name="categoria">
                    <option value="Arte">Arte</option>
                    <option value="Figuras">Figuras</option>
                    <option value="Gadgets">Gadgets</option>
                    <option value="Herramientas">Herramientas</option>
                    <option value="Hogar">Hogar</option>
                    <option value="Joyas">Joyas</option>
                    <option value="Jugetes">Jugetes</option>
                </select>
                <p>Descripcion:</p>
                <textarea name="descripcion" cols="30" rows="10"></textarea>
                <p>Cantidad:</p>
                <input type="number" name="stock">
                <p>URL imagen:</p>
                <input type="url" name="dir" placeholder="Solo google" onchange="imprimir_imagen()">
            </div>
            <button type="button" onclick="check()">Enviar</button>
        </main>
        <div style="height: 200px; width: 400px;">
            <img id=img_placeholder src="" style="height: 200px; width: 400px;" alt="No found">
        </div>
    <%- include('./templates/footer.ejs') %> 
    <script src="main.js"></script>
    <script src="admin.js"></script>
</body>